{
    "name": "lucenarenato/sail-ssl",
    "description": "Laravel 7 e php7.2 - Laravel Sail plugin to enable SSL (HTTPS) connection with Nginx.",
    "keywords": [
        "laravel",
        "sail",
        "docker",
        "ssl",
        "nginx"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lucenarenato/sail-ssl/issues",
        "source": "https://github.com/lucenarenato/sail-ssl"
    },
    "authors": [
        {
            "name": "Renato Lucena",
            "email": "cpdrenato@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^8.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lucenarenato\\SailSsl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lucenarenato\\SailSsl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucenarenato\\SailSsl\\SailSslServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "ajuchacko/sail-7.x": "1.x-dev",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.4|^9.0|^9.5"
    }
}
